<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro">

  <gazebo reference="${prefix}agv_link">
    <material>Gazebo/Blue</material>
    <mu1>0.5</mu1>
    <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}base_link">
    <material>Gazebo/Purple</material>
    <mu1>0.5</mu1>
    <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}link_1">
      <material>Gazebo/Yellow</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}link_2">
      <material>Gazebo/Red</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}link_3">
      <material>Gazebo/Orange</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}link_4">
      <material>Gazebo/Grey</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}link_5">
      <material>Gazebo/Green</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}link_6">
      <material>Gazebo/Blue</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}link_7">
      <material>Gazebo/Yellow</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}pan_link">
      <material>Gazebo/Black</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}xiao_arm1_link">
      <material>Gazebo/White</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}xiao_arm2_link">
      <material>Gazebo/Green</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <gazebo reference="${prefix}xiangji_link">
      <material>Gazebo/Orange</material>
      <mu1>0.5</mu1>
      <mu2>0.5</mu2>
  </gazebo>

  <!--ros control plugin-->
  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/iiwa</robotNamespace>
      <robotParam>/iiwa/robot_description</robotParam>
      <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
      <!--legacyModeNS>true</legacyModeNS-->
    </plugin>
  </gazebo>

  <!--move_base plugin -->
  <gazebo>
    <plugin name="base_controller" filename="libgazebo_ros_planar_move.so">
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>world</odometryTopic>
      <odometryFrame>world</odometryFrame>
      <odometryRate>20.0</odometryRate>
      <robotBaseFrame>agv_link</robotBaseFrame>
    </plugin>
  </gazebo>

</robot>