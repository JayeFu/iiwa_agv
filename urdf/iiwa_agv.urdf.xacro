  <joint name="${prefix}joint_a7" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="${prefix}link_6" />
    <child link="${prefix}link_7" />
    <axis xyz="0 0 1" />
    <limit lower="-3.0541" upper="3.0541" effort="50" velocity="2.356" />
